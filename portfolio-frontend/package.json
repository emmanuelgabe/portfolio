{
  "name": "portfolio-frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "version:generate": "node scripts/generate-version.js",
    "env:generate": "node scripts/generate-env.js",
    "prebuild": "npm run version:generate && npm run env:generate",
    "start": "npm run version:generate && ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "pretest": "npm run version:generate",
    "test": "ng test",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss,json}\"",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.16",
    "@angular/cdk": "^19.2.16",
    "@angular/common": "^19.2.16",
    "@angular/compiler": "^19.2.16",
    "@angular/core": "^19.2.16",
    "@angular/forms": "^19.2.16",
    "@angular/platform-browser": "^19.2.16",
    "@angular/platform-browser-dynamic": "^19.2.16",
    "@angular/router": "^19.2.16",
    "@angular/service-worker": "^19.2.16",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@popperjs/core": "^2.11.8",
    "@sentry/angular": "^8.55.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.1",
    "dompurify": "^3.3.1",
    "easymde": "^2.20.0",
    "lottie-web": "^5.13.0",
    "ng2-charts": "^5.0.4",
    "ngx-quill": "^27.1.2",
    "ngx-toastr": "^19.1.0",
    "prismjs": "^1.30.0",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-vitals": "^4.2.4",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.16",
    "@angular/cli": "^19.2.16",
    "@angular/compiler-cli": "^19.2.16",
    "@types/dompurify": "^3.0.5",
    "@types/jasmine": "^5.1.12",
    "@types/node": "^20.11.0",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "^5.12.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "~5.5.0"
  }
}
